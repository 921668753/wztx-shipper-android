<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/MatchMatch"
    android:background="@color/background"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <include layout="@layout/common_title"/>

    <ScrollView
        style="@style/MatchAuto"
        android:scrollbars="none">

        <LinearLayout
            style="@style/MatchWrap"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/ll_personal"
                style="@style/MatchWrap"
                android:orientation="vertical">

                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/realName"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_realName"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen41"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>

                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/bindingMobilePhone"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_bindingMobilePhone"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen26"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>

                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/IdNumber"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_idNumber"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen41"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>

                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/sex"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_sex"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen70"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_enterprise"
                style="@style/MatchWrap"
                android:orientation="vertical">

                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/companyName"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_companyName"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen65"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>

                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/registrationNumber"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_registrationNumber"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen20"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>


                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/legalPersonName"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_legalPersonName"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen38"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>


                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/legalPersonIdNumber"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_legalPersonIdNumber"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen38"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>


                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/phoneNumber"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_phoneNumber"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen38"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>


                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/address"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_address"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen100"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>

                <LinearLayout
                    style="@style/Match44"
                    android:background="@color/mainColor"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/WrapWrap"
                        android:layout_marginLeft="@dimen/dimen15"
                        android:gravity="center"
                        android:text="@string/operationIdNumber"
                        android:textColor="@color/hintcolors"
                        android:textSize="@dimen/textsize15" />

                    <TextView
                        android:id="@+id/tv_operationIdNumber"
                        style="@style/AutoWrap"
                        android:layout_marginLeft="@dimen/dimen28"
                        android:layout_marginRight="@dimen/dimen15"
                        android:text="--"
                        android:textColor="@color/textColor"
                        android:textSize="@dimen/textsize15" />
                </LinearLayout>
            </LinearLayout>

            <com.ruitukeji.zwbh.utils.myview.ChildListView xmlns:tools="http://schemas.android.com/tools"
                android:id="@+id/lv_img"
                style="@style/MatchAuto"
                android:layout_marginBottom="@dimen/dimen15"
                android:layout_marginTop="@dimen/dimen15"
                android:background="@color/mainColor"
                android:dividerHeight="@dimen/dimen15"
                android:overScrollMode="never"
                android:scrollbars="none"
                tools:listitem="@layout/item_imageview" />

        </LinearLayout>

    </ScrollView>
</LinearLayout>